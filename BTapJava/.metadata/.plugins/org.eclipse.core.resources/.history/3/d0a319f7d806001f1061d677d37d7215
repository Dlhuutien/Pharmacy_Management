package entity;

import java.util.Objects;

public class ThanhPhan {
	private String maTP;
	private String tenTP;
	private String chucNang;
	public ThanhPhan(String maTP, String tenTP, String chucNang) {
		super();
		this.maTP = maTP;
		this.tenTP = tenTP;
		this.chucNang = chucNang;
	}
	public ThanhPhan(String maTP) {
		super();
		this.maTP = maTP;
	}
	public ThanhPhan() {
		super();
	}
	@Override
	public String toString() {
		return "ThanhPhan [maTP=" + maTP + ", tenTP=" + tenTP + ", chucNang=" + chucNang + "]";
	}
	public String getMaTP() {
		return maTP;
	}
	public void setMaTP(String maTP) {
		this.maTP = maTP;
	}
	public String getTenTP() {
		return tenTP;
	}
	public void setTenTP(String tenTP) {
		this.tenTP = tenTP;
	}
	public String getChucNang() {
		return chucNang;
	}
	public void setChucNang(String chucNang) {
		this.chucNang = chucNang;
	}
	@Override
	public int hashCode() {
		return Objects.hash(maTP);
	}
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		ThanhPhan other = (ThanhPhan) obj;
		return Objects.equals(maTP, other.maTP);
	}
	
	
}
