package entity;

import java.util.Objects;

public class TaiKhoanNhanVien {
	private String taiKhoan;
	private String matKhau;
	private String maNV;
	public TaiKhoanNhanVien(String taiKhoan, String matKhau, String maNV) {
		super();
		this.taiKhoan = taiKhoan;
		this.matKhau = matKhau;
		this.maNV = maNV;
	}
	public TaiKhoanNhanVien(String maNV) {
		super();
		this.maNV = maNV;
		this.taiKhoan = maNV;
	}
	public TaiKhoanNhanVien() {
		super();
	}
	public String getTaiKhoan() {
		return taiKhoan;
	}
	public void setTaiKhoan(String taiKhoan) {
		this.taiKhoan = taiKhoan;
	}
	public String getMatKhau() {
		return matKhau;
	}
	public void setMatKhau(String matKhau) {
		this.matKhau = matKhau;
	}
	public String getmaNV() {
		return maNV;
	}
	public void setmaNV(String maNV) {
		this.maNV = maNV;
	}
	@Override
	public String toString() {
		return "TaiKhoanNhanVien [taiKhoan=" + taiKhoan + ", matKhau=" + matKhau + ", maNV=" + maNV + "]";
	}
	@Override
	public int hashCode() {
		return Objects.hash(taiKhoan);
	}
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		TaiKhoanNhanVien other = (TaiKhoanNhanVien) obj;
		return Objects.equals(taiKhoan, other.taiKhoan);
	}
}
